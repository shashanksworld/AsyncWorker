<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">

    <servlet>
					<servlet-name>dispatcher</servlet-name>
					<servlet-class> org.springframework.web.servlet.DispatcherServlet</servlet-class>
					
	</servlet>
			
	<servlet-mapping>
					<servlet-name>dispatcher</servlet-name>
					<url-pattern>*.do</url-pattern>
	</servlet-mapping>
    
    	<servlet>
		<servlet-name>AppCacheManager</servlet-name>
		<servlet-class>com.AppCacheManagerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AppCacheManager</servlet-name>
		<url-pattern>/appcachemanager</url-pattern>
	</servlet-mapping>
	<servlet>
    <servlet-name>Upload</servlet-name>
    <servlet-class>com.helper.Upload</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>Serve</servlet-name>
    <servlet-class>com.helper.Serve</servlet-class>
  </servlet>
 
  <servlet-mapping>
    <servlet-name>Upload</servlet-name>
    <url-pattern>/upload</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Serve</servlet-name>
    <url-pattern>/serve</url-pattern>
  </servlet-mapping>
  
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
